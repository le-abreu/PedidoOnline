<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<managed-bean>
		<managed-bean-name>produtoHandle</managed-bean-name>
		<managed-bean-class>br.com.empresa.handle.ProdutoHandler</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>photoCam</managed-bean-name>
		<managed-bean-class>br.com.empresa.handle.PhotoCamBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<converter>  
	   <converter-id>br.com.produto.bean.Produto</converter-id>  
	   <converter-class>br.com.empresa.bean.convert.ProdutoConvert</converter-class>  
	</converter>
</faces-config>
